<%= form_with model: @property, local: true do |f| %>
  <% if @property.errors.any? %>
  <div id="error_explanation">
     <h2>There were some errors:</h2>
     <ul>
      <% @property.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
  </div>
  <% end %>
<%= f.label :address %>
<%= f.text_field :address %>

<%= f.label :city %>
<%= f.text_field :city %>

<%= f.label :state %>
<%= f.text_field :state %>

<%= f.label :zip %>
<%= f.text_field :zip %>

<%= f.label :county %>
<%= f.text_field :county %>

<%= f.label :rent %>
<%= f.text_field :rent %>

<%= f.label :Landlord %>
<%= f.text_field :landlord %>

<%= f.submit %>

<% end %>